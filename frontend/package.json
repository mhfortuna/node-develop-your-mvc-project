{
  "name": "frontend",
  "version": "1.0.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test2": "jest --env=node",
    "test:ci:all": "jest --env=node",
    "test:watch": "jest --watch",
    "test:related": "jest --bail --findRelatedTests",
    "lint:js": "eslint . --ext .js",
    "lint:js:fix": "yarn run lint:js -- --fix",
    "lint:format": "prettier --write .",
    "lint:format:check": "prettier --check .",
    "prepare": "cd .. && husky install frontend/.husky",
    "precommit": "lint-staged",
    "prepush": "npm run lint:js && npm run lint:format:check && npm run test"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@html-eslint/eslint-plugin": "^0.9.0",
    "@html-eslint/parser": "^0.9.0",
    "@types/jest": "^26.0.20",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-babel": "^0.2.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^2.0.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.1.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3"
  },
  "dependencies": {
    "backend": "1.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "web-vitals": "^1.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
